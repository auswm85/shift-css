{
  "name": "Shift Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0a0a",
    "editor.foreground": "#e5e5e5"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#e5e7eb"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#e879f9"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#6ee7b7"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#c4b5fd"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css",
        "variable.css",
        "variable.argument.css"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#6ee7b7"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#ccfbf1"
      }
    },
    {
      "name": "Attribute Name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fdba74"
      }
    },
    {
      "name": "Attribute Value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#6ee7b7"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.separator"],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a3a3a3"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#67e8f9"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.other", "variable.other.constant"],
      "settings": {
        "foreground": "#fcd34d"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#fca5a5"
      }
    }
  ]
}
